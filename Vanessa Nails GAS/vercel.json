{
  "version": 2,
  "builds": [
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/seleccion-fecha",
      "dest": "/public/seleccion-fecha.html"
    },
    {
      "src": "/confirmacion",
      "dest": "/public/confirmacion.html"
    }
  ]
}